<script>
  import { gsap } from "gsap";
  import { ScrollTrigger } from "gsap/ScrollTrigger";

  gsap.registerPlugin(ScrollTrigger);

  let tl = gsap.timeline({
    scrollTrigger: {
      trigger: ".saks_container_2",
      start: "top 90%", //
      end: "bottom end",
      scrub: true, //
      markers: true,
    },
  });

  tl.from(".saks_container_2", {
    x: 900,
    y: -200,
    rotation: 30,
  });
</script>
<div class="saks_container_2"><div class="saks"></div></div>

<div class="col"></div>
<img class="stof" src="/img/img_svg/stofrolle.svg" alt="Tøm stofrolle" />
<img class="rolle" src="/img/img_svg/logo.svg" alt="Tøm stofrolle" />

<style>
  .col {
    height: 50px;
  }
  .stof {
    width: 100%;
  }
  .rolle {
    width: 100%;
  }
  .saks_container_2 {
    width: 200px;
    height: 100px;
  }
  .saks {
    width: 100%;
    height: 93%;
    background-size: contain;
    background-repeat: no-repeat;
    z-index: 10;
    background-image: url("/img/img_svg/saks.svg");
  }

  @media (min-width: 800px) {
    .col {
      height: 30px;
    }
  }
</style>
